{
    "name": "nextflow",
    "displayName": "Nextflow",
    "description": "Nextflow language support",
    "version": "0.1.0",
    "icon": "https://www.nextflow.io/img/nextflow-icon-128x128.png",
    "publisher": "pditommaso",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "nextflow",
            "aliases": ["Nextflow", "nextflow"],
            "extensions": [".nf"],
            "firstLine": "^#!.*\\bnextflow\\b",
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nextflow",
            "scopeName": "source.nextflow",
            "path": "./syntaxes/nextflow.tmLanguage.json"
        }, 
        {
            "scopeName": "source.nextflow-groovy",
            "path": "./syntaxes/groovy.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "nextflow",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}